    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.5",
    "@mui/material": "^7.3.5",
    "@mui/material-nextjs": "^7.3.5",
    "@mui/x-date-pickers": "^8.18.0",
    "@stripe/stripe-js": "^8.6.0",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.83.0",
    "@swc/helpers": "^0.5.17",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.6",
    "@types/react-dom": "^19.2.3",
    "@vercel/analytics": "^1.6.1",
    "dayjs": "^1.11.19",
    "next": "^16.0.7",
    "next-intl": "^4.5.6",
    "react": "^19.2.1",
