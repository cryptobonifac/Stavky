{
  "name": "stavky",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "type-check": "tsc --noEmit",
    "dev": "npm run type-check && next dev",
    "dev:skip-check": "next dev",
    "dev:with-webhooks": "concurrently \"npm run dev:skip-check\" \"stripe listen --forward-to localhost:3000/api/webhooks/stripe\" --names \"NEXT,STRIPE\" --prefix-colors \"blue,green\"",
    "dev:with-webhooks:help": "echo 'For sandbox mode, add STRIPE_SANDBOX_API_KEY=sk_test_... to .env.local, then run: node scripts/start-dev-with-webhooks.js'",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "db:push": "supabase db push",
    "db:reset": "supabase db reset",
    "db:local": "supabase start",
    "cleanup:dev": "node scripts/cleanup-dev.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cryptobonifac/Stavky.git"
  },
